<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd" version="3.1">
  <display-name>dht-routing </display-name>
  <enterprise-beans>
    <session>
      <ejb-name>RoutingBean</ejb-name>
      <env-entry>
        <env-entry-name>Key</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>20</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>Uri</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>http://ec2-107-20-52-80.compute-1.amazonaws.com:8080/dht-rest/dht/node</env-entry-value>
      </env-entry>
    </session>
  </enterprise-beans>
  <ejb-client-jar>dht-routingClient.jar</ejb-client-jar>
</ejb-jar>